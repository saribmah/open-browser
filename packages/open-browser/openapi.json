{
  "openapi": "3.1.1",
  "info": {
    "title": "Open Browser API",
    "description": "API documentation for Open Browser service",
    "version": "1.0.0"
  },
  "paths": {
    "/health": {
      "get": {
        "operationId": "getHealth",
        "description": "Health Check",
        "responses": {
          "200": {
            "description": "Service is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    },
                    "timestamp": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "status",
                    "timestamp"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "operationId": "getIndex",
        "description": "Service Information",
        "responses": {
          "200": {
            "description": "Service details",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "service": {
                      "type": "string"
                    },
                    "version": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "service",
                    "version"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/sandbox": {
      "get": {
        "operationId": "getSandbox",
        "description": "Get All Sandboxes",
        "responses": {
          "200": {
            "description": "Sandboxes retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "sandboxes": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string"
                          },
                          "provider": {
                            "type": "string",
                            "enum": [
                              "cloudflare",
                              "daytona",
                              "vercel"
                            ]
                          },
                          "status": {
                            "type": "string",
                            "enum": [
                              "pending",
                              "running",
                              "stopped",
                              "error"
                            ]
                          },
                          "url": {
                            "type": "string"
                          },
                          "createdAt": {
                            "type": "string"
                          },
                          "metadata": {
                            "type": "object",
                            "propertyNames": {
                              "type": "string"
                            },
                            "additionalProperties": {}
                          }
                        },
                        "required": [
                          "id",
                          "provider",
                          "status",
                          "createdAt"
                        ]
                      }
                    }
                  },
                  "required": [
                    "sandboxes"
                  ]
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "postSandbox",
        "description": "Create New Sandbox",
        "responses": {
          "200": {
            "description": "Sandbox created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "sandbox": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "provider": {
                          "type": "string",
                          "enum": [
                            "cloudflare",
                            "daytona",
                            "vercel"
                          ]
                        },
                        "status": {
                          "type": "string",
                          "enum": [
                            "pending",
                            "running",
                            "stopped",
                            "error"
                          ]
                        },
                        "url": {
                          "type": "string"
                        },
                        "createdAt": {
                          "type": "string"
                        },
                        "metadata": {
                          "type": "object",
                          "propertyNames": {
                            "type": "string"
                          },
                          "additionalProperties": {}
                        }
                      },
                      "required": [
                        "id",
                        "provider",
                        "status",
                        "createdAt"
                      ]
                    }
                  },
                  "required": [
                    "success"
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "error"
                  ]
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "url": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "GITHUB",
                      "ARXIV"
                    ]
                  },
                  "directory": {
                    "type": "string"
                  },
                  "sdkType": {
                    "type": "string",
                    "enum": [
                      "OPENCODE",
                      "CLAUDE_CODE"
                    ]
                  },
                  "provider": {
                    "type": "string",
                    "enum": [
                      "cloudflare",
                      "daytona",
                      "vercel"
                    ]
                  }
                },
                "required": [
                  "url",
                  "type",
                  "directory",
                  "sdkType",
                  "provider"
                ]
              }
            }
          }
        }
      }
    },
    "/sandbox/{id}": {
      "get": {
        "operationId": "getSandbox:id",
        "description": "Get Sandbox by ID",
        "responses": {
          "200": {
            "description": "Sandbox retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "provider": {
                      "type": "string",
                      "enum": [
                        "cloudflare",
                        "daytona",
                        "vercel"
                      ]
                    },
                    "status": {
                      "type": "string",
                      "enum": [
                        "pending",
                        "running",
                        "stopped",
                        "error"
                      ]
                    },
                    "url": {
                      "type": "string"
                    },
                    "createdAt": {
                      "type": "string"
                    },
                    "metadata": {
                      "type": "object",
                      "propertyNames": {
                        "type": "string"
                      },
                      "additionalProperties": {}
                    }
                  },
                  "required": [
                    "id",
                    "provider",
                    "status",
                    "createdAt"
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Sandbox not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "error"
                  ]
                }
              }
            }
          }
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "path",
            "name": "id",
            "required": true
          }
        ]
      },
      "delete": {
        "operationId": "deleteSandbox:id",
        "description": "Delete Sandbox",
        "responses": {
          "200": {
            "description": "Sandbox deleted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "sandbox": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "provider": {
                          "type": "string",
                          "enum": [
                            "cloudflare",
                            "daytona",
                            "vercel"
                          ]
                        },
                        "status": {
                          "type": "string",
                          "enum": [
                            "pending",
                            "running",
                            "stopped",
                            "error"
                          ]
                        },
                        "url": {
                          "type": "string"
                        },
                        "createdAt": {
                          "type": "string"
                        },
                        "metadata": {
                          "type": "object",
                          "propertyNames": {
                            "type": "string"
                          },
                          "additionalProperties": {}
                        }
                      },
                      "required": [
                        "id",
                        "provider",
                        "status",
                        "createdAt"
                      ]
                    }
                  },
                  "required": [
                    "success"
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Sandbox not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "error"
                  ]
                }
              }
            }
          }
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "path",
            "name": "id",
            "required": true
          }
        ]
      }
    }
  },
  "components": {}
}