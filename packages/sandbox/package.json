{
  "name": "sandbox",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "rm -rf ../../../sandbox/temp && bun --hot run ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "build:image:dev": "scripts/build.sh build dev",
    "generate:openapi": "bun run scripts/openapi.ts",
    "generate:client": "bun run scripts/generate.ts",
    "generate": "bun run generate:openapi && bun run generate:client"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/standard-validator": "^0.2.0",
    "@opencode-ai/sdk": "^1.0.80",
    "hono": "^4.10.6",
    "hono-openapi": "^1.1.1",
    "zod": "^4.1.12"
  }
}
